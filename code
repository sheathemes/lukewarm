<!-----------Tooltips Script-----------> 

<script>
    $(document).ready(function(){
        tippy('a[title]', { 
            theme: 'custom', 
            arrow: tippy.roundArrow, 
            zIndex: 9999999999, 
            placement: 'auto',
            maxWidth: 300, 
            content(reference) { 
                const title = reference.getAttribute('title'); 
                reference.removeAttribute('title'); return title; }, 
            
        });
    });
</script>
